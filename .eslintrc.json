{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "google",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "camelCase" : "warn",
        "prettier/prettier": "error",
        "no-unused-vars": "warn",
        "no-bitwise": "off"
    },
    "ignorePatterns": [
        "node_modules/"
    ]
}